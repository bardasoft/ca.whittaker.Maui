<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="ca.whittaker.Maui.Controls.Demo.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:ca.whittaker.Maui.Controls.Demo"
    xmlns:views="clr-namespace:ca.whittaker.Maui.Controls.Demo.Views"
    xmlns:strings="clr-namespace:ca.whittaker.Maui.Controls.Demo.Resources.Strings"    
    Shell.FlyoutBehavior="Flyout"
    Title="ca.whittaker.Maui.Controls.Demo">

    <TabBar>
        <Tab Title="Tab1">
            <ShellContent Title="UserProfilePage" ContentTemplate="{DataTemplate views:UserProfilePage}"></ShellContent>
            <ShellContent Title="LoginLogout" ContentTemplate="{DataTemplate views:LoginLogoutPage}"></ShellContent>
            <ShellContent Title="MauiPage1" ContentTemplate="{DataTemplate views:MauiPage1}"></ShellContent>
            <ShellContent Title="MauiPage2" ContentTemplate="{DataTemplate views:MauiPage2}"></ShellContent>
        </Tab>
    </TabBar>

</Shell>
